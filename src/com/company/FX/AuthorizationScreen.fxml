<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="375.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.FX.AuthorizationScreenController">
    <children>
      <ImageView fitHeight="389.0" fitWidth="600.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/dota2-wallpapers.seoplus.com_.ua-96.jpg" />
         </image></ImageView>
        <TextField fx:id="textFieldLogin" layoutX="224.0" layoutY="173.0" style="-fx-background-color: transparent; -fx-border-color: blue; -fx-text-fill: white;" />
        <PasswordField fx:id="passwordFieldPassword" layoutX="226.0" layoutY="222.0" onKeyPressed="#onPasswordFieldKeyPressed" style="-fx-background-color: transparent; -fx-border-color: blue; -fx-text-fill: white;">
         <cursor>
            <Cursor fx:constant="SE_RESIZE" />
         </cursor></PasswordField>
        <Label layoutX="191.0" layoutY="178.0" prefHeight="3.0" prefWidth="30.0" text="login:" textFill="#d7d7d7" />
        <Label layoutX="168.0" layoutY="227.0" text="password:" textFill="#e2dede" />
        <Button fx:id="buttonAuthorization" layoutX="248.0" layoutY="314.0" mnemonicParsing="false" onAction="#onButtonAuthorizationClick" style="-fx-background-color: transparent;" text="Authorization" textFill="#f8f5f5">
         <font>
            <Font size="14.0" />
         </font></Button>
    </children>
</AnchorPane>
